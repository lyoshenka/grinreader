<!DOCTYPE html>
<html lang="en">
  {% include '_head.twig' %}
  <body>
    <div id="loading" class="hide">loading</div>
    <div class="container-fluid">
      <div class="row-fluid">
        <div class="span2" id="sidebar">
          {% include '_sidebar.twig' %}
        </div>
        <div class="span10">
          {% include '_flashes.twig' %}
          <div class="page-header">
            <h1><a href="/">RSS Reader</a> <small>a google reader clone</small></h1>
          </div>
          <div class="js-feed-content">
            {% block content %}{% endblock %}
          </div>
          <div class="after-articles">
            <a class="js-scroll-to-top" href="javascript:;">The End</a>
          </div>
        </div>
      </div>
    </div>
    <script type="text/javascript" src="/js/jquery.min.js"></script>
    <script type="text/javascript" src="/js/bootstrap.js"></script>
    <script type="text/javascript" src="/js/mousetrap.min.js"></script>
    <script type="text/javascript" src="/js/mousetrap_help.js"></script>
    <script type="text/javascript" src="/js/script.js"></script>
  </body>
</html>
