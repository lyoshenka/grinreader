{% extends "layout.twig" %}
{% block content %}
  <h2>Feeds ({{ feeds.length }})</h2>
  <ul class="unstyled">
    {% for feed in feeds %}
      <li class="feed_list_item">
        <a href="/feed/{{ feed.id }}">{{ feed.name }}</a> ({{ feed.getUnreadCount() }})
      </li>
    {% endfor %}
  </ul>
{% endblock %}